<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="199.0" prefWidth="506.0" styleClass="root" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.PasswordCtrl">
   <stylesheets>
       <URL value="@/client/css/AddCardListStyle.css" />
   </stylesheets>
   <children>
      <VBox layoutX="29.0" layoutY="8.0" prefHeight="174.0" prefWidth="457.0">
         <children>
            <BorderPane prefHeight="82.0" prefWidth="427.0">
               <left>
                  <ImageView fitHeight="50.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@/client/images/bracket.png" />
                     </image>
                  </ImageView>
               </left>
               <right>
                  <ImageView fitHeight="50.0" fitWidth="28.0" pickOnBounds="true" preserveRatio="true" scaleX="-1.0" BorderPane.alignment="CENTER">
                     <image>
                        <Image url="@/client/images/bracket.png" />
                     </image>
                  </ImageView>
               </right>
               <center>
                  <PasswordField fx:id="password" prefHeight="43.0" prefWidth="375.0" promptText="Password" BorderPane.alignment="CENTER">
                     <styleClass>
                        <String fx:value="insert" />
                        <String fx:value="text-0" />
                     </styleClass>
                  </PasswordField>
               </center>
            </BorderPane>
            <HBox prefHeight="42.0" prefWidth="220.0">
               <children>
                  <Button fx:id="cancel" alignment="CENTER" mnemonicParsing="false" onAction="#cancel" prefHeight="25.0" prefWidth="108.0" text="Close" />
                  <VBox prefHeight="42.0" prefWidth="221.0" />
                  <Button fx:id="unlock" alignment="CENTER" mnemonicParsing="false" onAction="#unlock" prefHeight="27.0" prefWidth="143.0" text="Unlock" />
               </children>
            </HBox>
            <HBox layoutX="10.0" layoutY="92.0" prefHeight="42.0" prefWidth="220.0">
               <children>
                  <Button fx:id="changePass" alignment="CENTER" mnemonicParsing="false" onAction="#changePass" prefHeight="27.0" prefWidth="209.0" text="Change Password" />
                  <VBox prefHeight="42.0" prefWidth="99.0" />
                  <Button fx:id="removePass" alignment="CENTER" mnemonicParsing="false" onAction="#removePass" prefHeight="27.0" prefWidth="196.0" text="Remove Password" />
               </children>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
               <padding>
                  <Insets top="20.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
